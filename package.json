{
  "name": "object-parser-cli",
  "version": "0.0.1-alpha.0",
  "description": "Object preprocessor",

  "bin": {
    "opc": "object-parser-cli"
  },
  "files": [
    "object-parser-cli",
    "index.js",
    "lib"
  ],

  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "scripts": {
    "default": "jasminetea test --verbose --timeout 1000",

    "Developer commands": "",
      "start": "$(opc package scripts.default) --watch",

    "Travis-CI commands": "",
      "test": "$(opc package scripts.default) --cover --report",

    "Npm update via Travis-CI": "",
      "prepublish": "node -e \"if(process.env.TRAVIS){}else{process.exit(1)}\" && npm run compile || echo skip prepublish",
      
      "compile": "coffee --bare --compile index.coffee",
      "postcompile": "coffee --output lib --bare --compile src"
  },

  "dependencies": {
    "cheerio": "^0.19.0",
    "commander-file": "0.0.1",
    "lodash": "^3.9.1",
    "object-parser": "0.0.1-alpha.3"
  },
  "devDependencies": {
    "coffee-script": "^1.9.2",
    "ghooks": "^0.3.2",
    "jasminetea": "^0.2.0-alpha.3"
  },
  "globalDependencies": {
    "object-parser-cli": "0.0.0"
  },

  "keywords": [
    "lodash",
    "css-select",
    "cli",
    "json",
    "json5",
    "jsonml",
    "yaml",
    "html",
    "xml"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },

  "author": "59naga <i59naga@icloud.com> (http://berabou.me/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/59naga/object-parser-cli.git"
  },
  "bugs": {
    "url": "https://github.com/59naga/object-parser-cli/issues"
  },
  "homepage": "https://github.com/59naga/object-parser-cli"
}
